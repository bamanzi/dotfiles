<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Ubuntu Packages (by name)</os:ShortName>
<os:Description>Ubuntu Packages by name)</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://packages.ubuntu.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://packages.ubuntu.com/search">
  <os:Param name="keywords" value="{searchTerms}"/>
  <os:Param name="searchon" value="names"/>
  <os:Param name="suite" value="all"/>
  <os:Param name="section" value="all"/>
</os:Url>
</SearchPlugin>