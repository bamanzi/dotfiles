<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Debian Popularity Contest Statistics</os:ShortName>
<os:Description>Debian Popularity Contest Statistics</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://qa.debian.org/popcon-graph.php</SearchForm>
<os:Url type="text/html" method="GET" template="https://qa.debian.org/popcon-graph.php">
  <os:Param name="packages" value="{searchTerms}"/>
  <os:Param name="from_date" value="2010-01-01"/>
  <os:Param name="show_vote" value="on"/>
</os:Url>
</SearchPlugin>
